<h1>Countries</h1>

<p><a href="/new_country">Add a New Country</a></p>

<table>
  <thead>
    <tr>
      <th>Title</th>
    
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @countries.each do |country| %>
    <tr>
          <td><%= country.title %></td>
              <% if session['uid'] %>
        <td><a href="/users/<%= user.id %>">Image</a></td>
      <% end %>
          <td><a href="/countries/<%= country.id %>">Show</a>
        </td>
         <% if session['uid'] %>
        <td><a href="/users/<%= user.id %>/edit">Edit</a></td>
        <td><a href="/users/<%= user.id %>/destroy">Destroy</a></td>
      <% end %>
    </tr>
  <% end %>
  </tbody>
</table>

